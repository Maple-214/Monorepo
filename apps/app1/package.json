    "react-router-dom": "^7.8.1",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  },
  "devDependencies": {
    "@anmx/vite-plugin-preamble": "1.0.0-beta.1",
    "@babel/preset-react": "^7.24.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/tailwindcss": "^3.1.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "http-server": "^14.1.1",
    "postcss": "^8.5.6",
